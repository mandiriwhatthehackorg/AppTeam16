<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mandiri.whatthehack.com.whatthehack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.ChatActivity" />
        <activity android:name=".views.activities.SignInActivity" />
        <activity android:name=".views.activities.InvestasiActivity" />
        <activity android:name=".views.activities.KontakFinansialActivity" />
        <activity android:name=".views.activities.CekTipeActivity"></activity>
    </application>

</manifest>